{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "worker": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-console": "off",
    "no-debugger": "warn",
    "prefer-const": "error",
    "no-var": "error",
    "prefer-arrow-callback": "error",
    "arrow-spacing": "error",
    "eqeqeq": ["error", "always"],
    "curly": ["error", "all"],
    "no-eval": "error",
    "no-implied-eval": "error",
    "indent": "off",
    "quotes": "off",
    "semi": "off",
    "no-duplicate-imports": "error"
  },
  "globals": {
    "self": "readonly",
    "importScripts": "readonly",
    "WebAssembly": "readonly"
  }
}